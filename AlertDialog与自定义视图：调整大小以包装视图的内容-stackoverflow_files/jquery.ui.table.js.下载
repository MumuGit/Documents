$.fn.table = function(params){
    var options = {
        hoverColor   : '#e4dddd'
    };
    $.extend(options, params);
    var arr = [];
  	$(this).find('tr').each(function(){
  	        var td =$(this).find('td');
            $(this).hover(function(){
               arr = [];
               td.each(function(i){
               		arr[i] = $(this).css('backgroundColor');
               		$(this).css('backgroundColor', options.hoverColor );
               });
            },function(){
 				td.each(function(i){
               		$(this).css('backgroundColor', arr[i] );
                });
            });
    });
};
$(function(){
	$(".table_1_1").table();
});