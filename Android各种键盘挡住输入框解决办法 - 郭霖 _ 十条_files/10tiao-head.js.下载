function changeSearchType(obj,type){
	$('#id-drapdown-btn').html($(obj).text()+'<span class="caret"></span>');
	$('#id-type').attr("value",type);
};
function searchWord(page){
	$('#id-page').attr("value",page);
	$('#id-search').submit();
};